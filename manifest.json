{
	"manifest_version": 2,
	"name": "YouTube Channel Homepage Set to Videos",
	"version": "1.1.3.2",
	
	"content_scripts":
	[
		{
			"js": ["js/content_script.js"],
			"matches": ["*://www.youtube.com/*", "*://m.youtube.com/*", "*://apis.google.com/*/hovercard/*src=youtube*"],			
			"all_frames": true,
			"run_at": "document_end"
		}
	]
}